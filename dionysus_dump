-- Name: pieces_id_seq; Type: SEQUENCE; Schema: public; Owner: dionysus
--

CREATE SEQUENCE pieces_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MAXVALUE
    NO MINVALUE
    CACHE 1;


ALTER TABLE public.pieces_id_seq OWNER TO dionysus;

--
-- Name: pieces_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: dionysus
--

ALTER SEQUENCE pieces_id_seq OWNED BY pieces.id;


--
-- Name: pieces_id_seq; Type: SEQUENCE SET; Schema: public; Owner: dionysus
--

SELECT pg_catalog.setval('pieces_id_seq', 3, true);


