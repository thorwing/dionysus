ALTER TABLE public.activity_feeds OWNER TO dionysus;

--
-- Name: activity_feeds_id_seq; Type: SEQUENCE; Schema: public; Owner: dionysus
--

CREATE SEQUENCE activity_feeds_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MAXVALUE
    NO MINVALUE
    CACHE 1;


ALTER TABLE public.activity_feeds_id_seq OWNER TO dionysus;

--
-- Name: activity_feeds_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: dionysus
--

ALTER SEQUENCE activity_feeds_id_seq OWNED BY activity_feeds.id;


