--
-- Name: active_admin_comments; Type: TABLE; Schema: public; Owner: dionysus; Tablespace: 
--

CREATE TABLE active_admin_comments (
    id integer NOT NULL,
    resource_id character varying(255) NOT NULL,
    resource_type character varying(255) NOT NULL,
    author_id integer,
    author_type character varying(255),
    body text,
    created_at timestamp without time zone NOT NULL,
    updated_at timestamp without time zone NOT NULL,
    namespace character varying(255)
);


ALTER TABLE public.active_admin_comments OWNER TO dionysus;

--
-- Name: activity_feeds; Type: TABLE; Schema: public; Owner: dionysus; Tablespace: 
--

CREATE TABLE activity_feeds (
